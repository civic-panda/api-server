{
  "name": "my-node-app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "rimraf build && tsc -p tsconfig.json",
    "postinstall": "npm run build",
    "start:dev": "nodemon",
    "start": "NODE_ENV=production node build/index.js",
    "clone-congress": "if cd congress-data; then git pull; else git clone git://github.com/unitedstates/congress-legislators.git congress-data; fi",
    "convert-congress": "yaml2json congress-data/legislators-current.yaml > data/legislators-current.json",
    "load-data": "npm run clone-congress && npm run convert-congress"
  },
  "dependencies": {
    "@types/body-parser": "0.0.33",
    "@types/cors": "0.0.33",
    "@types/express": "^4.0.34",
    "@types/lodash": "^4.14.43",
    "@types/morgan": "^1.7.32",
    "@types/node-fetch": "^1.6.6",
    "@types/request": "0.0.36",
    "body-parser": "^1.15.2",
    "cors": "^2.8.1",
    "express": "4.10.7",
    "lodash": "^4.17.2",
    "morgan": "^1.7.0",
    "ms": "^0.7.2",
    "node-fetch": "^1.6.3",
    "nodemon": "^1.11.0",
    "qs": "^6.3.0",
    "request": "^2.79.0",
    "rimraf": "^2.5.4",
    "ts-node": "^1.7.0",
    "typescript": "^2.1.1",
    "yamljs": "^0.2.8"
  },
  "devDependencies": {},
  "engines": {
    "node": "7.2.1",
    "npm": "4.0.3"
  }
}
