{
  "name": "my-node-app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "rimraf build && tsc -p tsconfig.json && npm run copy-public && browserify build/client/index.js -o build/public/js/bundle.js",
    "copy-public": "cp -R src/public build/public && cp -R node_modules/tinymce build/public/js",
    "postinstall": "npm run build && npm run load-data",
    "watch-tsc": "tsc -p tsconfig.json -w",
    "watch-client": "watchify build/client/index.js -o build/public/js/bundle.js",
    "dev": "nodemon",
    "start": "NODE_ENV=production node build/index.js",
    "migrate": "knex migrate:latest --knexfile build/db/knexfile.js",
    "rollback": "knex migrate:rollback --knexfile build/db/knexfile.js",
    "seed": "knex seed:run --knexfile build/db/knexfile.js",
    "generate-db-types": "schemats generate -c postgres://mattcasey@localhost/actonthis -o src/db/schema.ts",
    "clone-data": "if cd congress-data; then git pull; else git clone git://github.com/unitedstates/congress-legislators.git congress-data; fi",
    "convert-data": "node scripts/convert-data.js",
    "load-data": "npm run clone-data && npm run convert-data"
  },
